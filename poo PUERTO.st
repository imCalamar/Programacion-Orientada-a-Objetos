Object subclass: #Alquiler	instanceVariableNames: 'cliente fechaInicio fechaFinal amarre barco monto'	classVariableNames: ''	poolDictionaries: ''	category: 'poo PUERTO'!!Alquiler methodsFor: 'inicializador' stamp: 'Anonymous 5/12/2018 21:19'!crearCliente: unCliente fechaFinal:unaFecha amarre:unAmarre barco:unBarcocliente:=unCliente.fechaInicio := Date today.fechaFinal:=unaFecha.amarre :=unAmarre.barco:=unBarco.monto:=-1.! !!Alquiler methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 11:39'!getBarco^barco.! !!Alquiler methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 11:34'!getCliente^cliente.! !!Alquiler methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 11:35'!getfechaInicio^fechaInicio .! !!Alquiler methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 11:37'!getfechaFinal^fechaFinal .! !!Alquiler methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 11:37'!getAmarre^amarre  .! !!Alquiler methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 11:41'!setfechaFinal: unaFechafechaFinal:=unaFecha  .! !!Alquiler methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 11:43'!setBarco: unBarcobarco:= unBarco   .! !!Alquiler methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 11:42'!setAmarre: unaAmarreamarre:=unaAmarre   .! !!Alquiler methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 11:40'!setCliente: unclientecliente :=uncliente .! !!Alquiler methodsFor: 'aplicacion' stamp: 'Anonymous 5/12/2018 21:18'!calcularMonto| montos diasOcupados unBarco|diasOcupados:= fechaFinal subtractDate: fechaInicio.unBarco := self getBarco .montos:= ((unBarco modulo)*diasOcupados )+2400.monto:= montos.^monto.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alquiler class	instanceVariableNames: ''!!Alquiler class methodsFor: 'constructor' stamp: 'Anonymous 4/20/2018 11:53'!crearCliente: unCliente fechaFinal:unaFecha amarre:unAmarre barco:unbarco|x|x:=self new.x crearCliente: unCliente fechaFinal:unaFecha  amarre:unAmarre   barco: unbarco.^x.! !Object subclass: #Barco	instanceVariableNames: 'matricula eslora anio'	classVariableNames: ''	poolDictionaries: ''	category: 'poo PUERTO'!!Barco methodsFor: 'inicializador' stamp: 'Anonymous 4/20/2018 10:16'!crearMatricula:unaMatricula eslora:unaEslora anio:unAniomatricula :=unaMatricula .eslora :=unaEslora.anio:=unAnio.! !!Barco methodsFor: 'aplicacion' stamp: 'Anonymous 4/26/2018 07:51'!modulo|modulo|modulo:=10*eslora.^modulo.! !!Barco methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 09:51'!getEslora^eslora  .! !!Barco methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 09:52'!getAnio^anio .! !!Barco methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 09:51'!getMatricula^matricula .! !!Barco methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 09:59'!setEslora:unaEsloraeslora :=unaEslora.! !!Barco methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 09:53'!setMatricula: unaMatriculamatricula :=unaMatricula.! !!Barco methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 09:54'!setAnio:unAnioanio :=unAnio.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Barco class	instanceVariableNames: ''!!Barco class methodsFor: 'constructor' stamp: 'Anonymous 4/20/2018 10:23'!CrearMatricula: unaMatricula eslora:unaEslora anio:unAnio|x|x:=self new.x crearMatricula:unaMatricula   eslora:unaEslora   anio:unAnio .^x .! !Object subclass: #Cliente	instanceVariableNames: 'nombre apellido dni telefono correo'	classVariableNames: ''	poolDictionaries: ''	category: 'poo PUERTO'!!Cliente methodsFor: 'modificadores' stamp: 'Anonymous 4/8/2018 02:16'!setApellido:capellido :=c.! !!Cliente methodsFor: 'modificadores' stamp: 'Anonymous 4/20/2018 10:58'!setCorreo: unCorreocorreo :=unCorreo ! !!Cliente methodsFor: 'modificadores' stamp: 'Anonymous 4/8/2018 02:14'!setDni:adni:=a.! !!Cliente methodsFor: 'modificadores' stamp: 'Anonymous 4/8/2018 02:18'!setTelefono:dteléfono :=d.! !!Cliente methodsFor: 'modificadores' stamp: 'Anonymous 4/8/2018 02:14'!setNombre:bnombre :=b.! !!Cliente methodsFor: 'inicializador' stamp: 'Anonymous 4/20/2018 10:59'!INICIARdni:a nombre:b apellido:c telefono:d  corre:unCorreodni:=a.nombre :=b.apellido :=c.telefono :=d.correo :=unCorreo.! !!Cliente methodsFor: 'visualisadores' stamp: 'Anonymous 4/8/2018 02:11'!getApellido^apellido .! !!Cliente methodsFor: 'visualisadores' stamp: 'Anonymous 4/8/2018 02:12'!getTelefono^teléfono  .! !!Cliente methodsFor: 'visualisadores' stamp: 'Anonymous 4/8/2018 02:13'!getDni^dni .! !!Cliente methodsFor: 'visualisadores' stamp: 'Anonymous 4/8/2018 02:11'!getNombre^nombre.! !!Cliente methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 10:57'!getCorreo^correo.! !!Cliente methodsFor: 'aplicacion' stamp: 'Anonymous 5/12/2018 21:22'!igual: unNombre^(nombre=unNombre ).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Cliente class	instanceVariableNames: ''!!Cliente class methodsFor: 'constructor' stamp: 'Anonymous 4/20/2018 11:01'!CREARdni:a nombre:b apellido:c telefono:d correo:e |x|x:=self new.x INICIARdni:a nombre:b apellido:c telefono:d  corre:e.^x. ! !Object subclass: #Puerto	instanceVariableNames: 'colAmarres colAlquileres base'	classVariableNames: ''	poolDictionaries: ''	category: 'poo PUERTO'!!Puerto methodsFor: 'aplicacion' stamp: 'Anonymous 5/12/2018 22:23'!buscarAlquiler: unNombre|x|x:= colAlquileres detect: [ :unAlquiler |(unAlquiler getCliente)igual:unNombre  ].^x.! !!Puerto methodsFor: 'aplicacion' stamp: 'Anonymous 5/12/2018 22:21'!registrarAlquiler: unBarco cliente: unCliente fechaFin: unaFecha	| a b c |	a :=self generarAmarre .	b := Alquiler new.	b crearCliente: unCliente fechaFinal: unaFecha amarre: a barco: unBarco.	c:=b calcularMonto.	colAlquileres add: b.	^c.! !!Puerto methodsFor: 'aplicacion' stamp: 'Anonymous 5/12/2018 21:51'!generarAmarre|x|x:=colAmarres  at:2.^x.! !!Puerto methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 12:51'!getColAmarre^colAmarres .! !!Puerto methodsFor: 'visualisadores' stamp: 'Anonymous 4/20/2018 12:46'!getColAlquiler^colAlquileres.! !!Puerto methodsFor: 'inicializador' stamp: 'Anonymous 5/12/2018 22:18'!iniciarPuerto| contador|colAmarres:= OrderedCollection new.contador :=1.[ contador < 12 ]whileTrue:[colAmarres add: contador.	contador :=contador+1].colAlquileres :=OrderedCollection  new.base:=2400.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Puerto class	instanceVariableNames: ''!!Puerto class methodsFor: 'constructor' stamp: 'Anonymous 4/20/2018 14:18'!crearpuerto|x|x:=self new.x iniciarPuerto.^x.! !